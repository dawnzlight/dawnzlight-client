        "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "chromatic": "chromatic --exit-zero-on-changes"
    },
    "dependencies": {
        "next": "14.2.5",
        "react": "^18",
        "react-dom": "^18"
    },
    "devDependencies": {
        "@chakra-ui/next-js": "^2.2.0",
