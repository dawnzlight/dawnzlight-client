        "chromatic": "^11.7.1",
        "eslint": "^8",
        "eslint-config-next": "14.2.5",
        "eslint-plugin-next-on-pages": "^1.13.2",
        "eslint-plugin-storybook": "^0.8.0",
        "framer-motion": "^11.3.30",
        "hygen": "^6.2.11",
        "jotai": "^2.9.3",
        "prettier": "^3.3.3",
        "storybook": "^8.2.9",
        "typescript": "^5",
        "vercel": "^37.1.1",
        "wrangler": "^3.72.2"
    },
    "volta": {
        "node": "22.7.0",
        "npm": "10.8.2"
    }
