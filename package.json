{
    "name": "dawnzlight-client",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint && prettier --check .",
        "format": "prettier . --write",
        "pages:build": "npx @cloudflare/next-on-pages",
